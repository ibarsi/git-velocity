{
    "presets": [
        "es2015",
        "es2016",
        "es2017"
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }]
    ],
    "sourceMaps": true,
    "retainLines": true
}
